{
    "name": "nerdery/documentation-bundle",
    "description": "Generate Bundle Documentation for Symfony2",
    "keywords": ["assets", "compression", "minification"],
    "homepage": "https://github.com/symfony/AsseticBundle",
    "type": "symfony-bundle",
    "license": "BSD-2-Clause",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Maxwell Vandervelde",
            "email": "Maxwell.Vandervelde@Nerdery.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/symfony": ">=2.1.0",
        "knplabs/knp-markdown-bundle": "1.2.0"
    },
    "autoload": {
        "psr-0": { "Nerdery\\DocumentationBundle": "" }
    },
    "target-dir": "Nerdery/DocumentationBundle"
}
